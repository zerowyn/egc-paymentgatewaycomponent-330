<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry
		location="D://repository//repository//org//postgresql//postgresql//42.1.4//postgresql-42.1.4.jar" />
	<context id="context" targetRuntime="MyBatis3"
		defaultModelType="conditional">
		<property name="autoDelimitKeywords" value="true" />
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="Example$" />
			<property name="replaceString" value="Criteria" />
		</plugin>
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
		<plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="org.postgresql.Driver"
			connectionURL="jdbc:postgresql://192.168.0.157:2018/hdsc_db" userId="pgc"
			password="pgc_password" />
		<javaModelGenerator targetPackage="com.eg.egsc.scp.paygateway.mapper.entity"
			targetProject="egc-paymentgatewaycomponent-dao/src/main/java" />
		<sqlMapGenerator targetPackage="com.eg.egsc.scp.paygateway.mapper"
			targetProject="egc-paymentgatewaycomponent-dao/src/main/java" />
		<javaClientGenerator targetPackage="com.eg.egsc.scp.paygateway.mapper"
			targetProject="egc-paymentgatewaycomponent-dao/src/main/java" type="XMLMAPPER" />
		<table schema="pgc" tableName="%" />
	</context>
</generatorConfiguration>